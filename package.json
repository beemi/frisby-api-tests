  "keywords": [
    "testing",
    "api",
    "REST",
    "jasmine",
    "bdd",
    "frisby"
  ],
  "engines": {
    "node": ">= 8.0.0"
  },
  "author": {
    "name": "Raja Beemi",
    "email": "beemi.raja@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "frisby": "^2.1.3",
    "jest": "^26.6.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/",
      "/node_modules/"
    ],
    "verbose": false
  },
  "dependencies": {
    "config": "^3.3.2"
  }
}
